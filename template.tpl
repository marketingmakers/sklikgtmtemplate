// Require the necessary APIs
const logToConsole = require('logToConsole');
const injectScript = require('injectScript');
const queryPermission = require('queryPermission');
const setInWindow = require('setInWindow');

// If the script loaded successfully, log a message and signal success
const onSuccess = () => {
  logToConsole('Skript Sklik' + data.typ + 'se načetl úspěšně.');
  data.gtmOnSuccess();
};

// If the script fails to load, log a message and signal failure
const onFailure = () => {
  logToConsole('Při načítání skriptu Sklik' + data.typ + 'došlo k chybě.');
  data.gtmOnFailure();
};

// Seznam retargeting
if (data.typ == "rtgt") {   
	if (data.rtgtid) { setInWindow('seznam_retargeting_id', data.rtgtid*1, true); } 
    if (data.itemId) { setInWindow('seznam_itemId', data.itemId, true);  }
    if (data.pagetype) { setInWindow('seznam_pagetype', data.pagetype, true); }  
    if (data.category) { setInWindow('seznam_category', data.category, true); } 

let url = 'https://c.imedia.cz/js/retargeting.js';
if (queryPermission('inject_script', url)) {
  injectScript(url, onSuccess, onFailure);
}
  
logToConsole('Posíláme data o:' + data.typ + 
    'seznam_retargeting_id:' +data.rtgtid+
    ', seznam_itemId:' + data.itemId +
    ', seznamPagetype:' + data.Pagetype +
    ', seznam_category:' +data.category);  
  
} else if (data.typ == "konverze") { // Seznam konverze
	if (data.cId) {  setInWindow('seznam_cId', data.cId*1, true); } 
    if (data.value) { setInWindow('seznam_value', data.value*1, true);  }
  
let url = 'https://www.seznam.cz/rs/static/rc.js';
if (queryPermission('inject_script', url)) {
  injectScript(url,onSuccess,onFailure);
}
  
logToConsole('Posíláme data o:' + data.typ + 
    'seznam_cId:' +data.cId+
    ', seznam_value:' + data.value);  
  
}


